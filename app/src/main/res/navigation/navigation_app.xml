<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_app"
    android:label="@string/title_doing"
    app:startDestination="@id/navigation_create_task">

    <fragment
        android:id="@+id/navigation_create_task"
        android:name="com.example.resultactcal.CreateTaskFragment"
        android:label="@string/title_create_task"
        tools:layout="@layout/fragment_create_task" />
    <fragment
        android:id="@+id/navigation_to_do"
        android:name="com.example.resultactcal.ToDoFragment"
        android:label="@string/title_to_do"
        tools:layout="@layout/fragment_to_do" />
    <fragment
        android:id="@+id/navigation_doing"
        android:name="com.example.resultactcal.DoingFragment"
        android:label="@string/title_doing"
        tools:layout="@layout/fragment_doing" />
    <fragment
        android:id="@+id/navigation_done"
        android:name="com.example.resultactcal.DoneFragment"
        android:label="@string/title_done"
        tools:layout="@layout/fragment_done" />
</navigation>